{
    "env": {
        "browser": true,
        "node": true,
        "es2020": true
    },
    "extends": [
        "plugin:@wordpress/eslint-plugin/esnext",
        "plugin:@typescript-eslint/recommended",
        "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
    ],
    "rules": {
      "prettier/prettier": "warn"
    }
}
